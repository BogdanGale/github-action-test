{
  "branches": ["features/semantic_release"],
  "plugins": [
    "@semantic-release/github",
    "@semantic-release/exec",
    [
      "@semantic-release/commit-analyzer",
      {
        "preset": "angular",
        "releaseRules": [
          {
            "type": "feat",
            "release": "patch"
          },
          {
            "type": "fix",
            "release": "patch"
          }
        ]
      }
    ]
  ]
}